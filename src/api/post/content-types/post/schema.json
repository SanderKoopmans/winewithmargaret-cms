{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.content",
        "content.header",
        "content.spacer-32",
        "content.spacer-64"
      ]
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "SEO_social": {
      "type": "component",
      "repeatable": true,
      "component": "shared.meta-social"
    },
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Excerpt": {
      "type": "text"
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "PageType": {
      "type": "component",
      "repeatable": false,
      "component": "page-settings.page-type"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "post"
    }
  }
}
