{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageType": {
      "type": "dynamiczone",
      "components": [
        "page-settings.data-type"
      ]
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.content",
        "content.header",
        "content.spacer-32",
        "content.spacer-64"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "seo_social": {
      "type": "component",
      "repeatable": true,
      "component": "shared.meta-social"
    },
    "title": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "post"
    },
    "Slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
